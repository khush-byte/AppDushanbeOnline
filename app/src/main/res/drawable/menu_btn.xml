<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item >
        <shape
            android:shape="rectangle">
            <solid android:color="#F6F6F6"/>
        </shape>
    </item>
    <item android:right="0dp" android:left="0dp" android:top="0.4dp">
        <shape
            android:shape="rectangle">
            <gradient
                android:endColor="#87BCCC"
                android:centerColor="#5F919F"
                android:startColor="#476770"
                android:angle="90"/>
            <stroke
                android:width="0.1dp"
                android:color="#3CA6A6A6" />
        </shape>
    </item>
</layer-list>